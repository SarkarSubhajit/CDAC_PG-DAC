package org.cdac.utils;

public class Entry {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		LinkedList<String> list = new LinkedList<>();
		
		list.add("Hello");
		list.add("World");
		list.add("Java");
		
		String str = list.getFirst();
		
		while(str != null) {
			System.out.println(str);
			str = list.getNext();
			str = list.getPrevious();
		}
		
	}

}
