package DSA;

public class Main {

	public static void main(String[] args) {
		
		int num = 5;
		Patterns pattern = new Patterns();
		pattern.square(num);
		System.out.println("==================================================");
		pattern.rightAngledTriangle(num);
		System.out.println("==================================================");
		pattern.rightAngledNumberTriangle(num);
		System.out.println("==================================================");
		pattern.rightAngledRowsNumberTriangle();
		System.out.println("==================================================");
		pattern.invertedRightPyramid();
		System.out.println("==================================================");
		pattern.invertedRightNumerPyramid();
		System.out.println("==================================================");
		pattern.starPyramid();
		System.out.println("==================================================");
		pattern.invertedStarPyramid();
		System.out.println("==================================================");
		System.out.println("==================================================");
		

	}

}
