package DSA;

public class Main {

	public static void main(String[] args) {
		
		Patterns pattern = new Patterns();
		pattern.square();
		System.out.println("==================================================");
		pattern.rightAngledTriangle();
		System.out.println("==================================================");
		pattern.rightAngledNumberTriangle();
		System.out.println("==================================================");
		pattern.rightAngledRowsNumberTriangle();
		System.out.println("==================================================");
		pattern.invertedRightPyramid();
		System.out.println("==================================================");
		pattern.invertedRightNumerPyramid();
		System.out.println("==================================================");
		pattern.startPyramid();
		System.out.println("==================================================");
		System.out.println("==================================================");
		System.out.println("==================================================");
		

	}

}
