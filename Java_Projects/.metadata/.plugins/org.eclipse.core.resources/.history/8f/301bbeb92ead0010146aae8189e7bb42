package DSA;

public class Main {

	public static void main(String[] args) {
		
		int num = 1;
		Patterns pattern = new Patterns();
		pattern.square(num);
		System.out.println("==================================================");
		pattern.rightAngledTriangle(num);
		System.out.println("==================================================");
		pattern.rightAngledNumberTriangle(num);
		System.out.println("==================================================");
		pattern.rightAngledRowsNumberTriangle(num);
		System.out.println("==================================================");
		pattern.invertedRightPyramid(num);
		System.out.println("==================================================");
		pattern.invertedRightNumerPyramid(num);
		System.out.println("==================================================");
		pattern.starPyramid(num);
		System.out.println("==================================================");
		pattern.invertedStarPyramid(num);
		System.out.println("==================================================");
		pattern.diamondStarPattern(num);
		System.out.println("==================================================");
		pattern.halfDiamondStarPattern(num);
		System.out.println("==================================================");
		pattern.binaryNumberTriangle(num);
		System.out.println("==================================================");
		pattern.numberCrownPattern(num);
		System.out.println("==================================================");
		pattern.increasingNumberTriangle(num);
		System.out.println("==================================================");
		pattern.increasingLetterTriangle(num);
		System.out.println("==================================================");
		pattern.reverseLetterTriangle(num);
		System.out.println("==================================================");
		pattern.alphaRampPattern(num);
		System.out.println("==================================================");
		pattern.letterPyramid(num);
		System.out.println("==================================================");
		pattern.alphaTrianglePattern(num);
		System.out.println("==================================================");
		pattern.symmetricVoidPattern(num);
		System.out.println("==================================================");
		pattern.symmetricButterflyPattern(num);
		System.out.println("==================================================");
		pattern.hollowRectangle(num);
		System.out.println("==================================================");
		pattern.theNumberPattern(num);
		
		
		

	}

}
